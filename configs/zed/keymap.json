[
  // Standard macOS bindings
  {
    "use_key_equivalents": true,
    "bindings": {
      "cmd-;": "dev::OpenKeyContextView", // <3
      "cmd-|": "editor::RestartLanguageServer",
      "f4": "debugger::Start",
      "shift-f5": "debugger::Stop",
      "shift-cmd-f5": "debugger::RerunSession",
      "f6": "debugger::Pause",
      "f7": "debugger::StepOver",
      "ctrl-f11": "debugger::StepInto",
      "shift-f11": "debugger::StepOut",
      "ctrl-n": "menu::SelectNext",
      "ctrl-p": "menu::SelectPrevious",
      "ctrl-y": "menu::Confirm",
      "enter": "menu::Confirm",
      "ctrl-enter": "menu::SecondaryConfirm",
      "ctrl-c": "menu::Cancel",
      "escape": "menu::Cancel",
      "cmd-shift-w": "workspace::CloseWindow",
      "shift-escape": "workspace::ToggleZoom",
      "cmd-=": ["zed::IncreaseBufferFontSize", { "persist": false }],
      "cmd--": ["zed::DecreaseBufferFontSize", { "persist": false }],
      "cmd-0": ["zed::ResetBufferFontSize", { "persist": false }],
      "cmd-,": "zed::OpenSettings",
      "cmd-.": "zed::OpenKeymap",
      "cmd-q": "zed::Quit",
      "cmd-m": "zed::Minimize",
      "ctrl-cmd-z": "edit_prediction::RateCompletions",
      "ctrl-cmd-l": "lsp_tool::ToggleMenu"
    }
  },
  {
    "context": "Editor",
    "use_key_equivalents": true,
    "bindings": {
      "backspace": "editor::Backspace",
      "tab": "editor::Tab",
      "shift-tab": "editor::Backtab",
      "cmd-v": "editor::Paste",
      "ctrl-cmd-space": "editor::ShowCharacterPalette",
      "cmd-alt-g b": "git::Blame",
      "cmd-alt-g m": "git::OpenModifiedFiles",
      "f9": "editor::ToggleBreakpoint",
      "shift-f9": "editor::EditLogBreakpoint"
    }
  },
  {
